{
  "name": "pontocooper",
  "version": "2.0.0",
  "author": "Cesar HenriQ",
  "description": "Aplicativo de ponto CooperSystem",
  "buildInfo": [
    "Nova vers√£o base do aplicativo"
  ],
  "homepage": "https://portal.coopersystem.com.br/",
  "scripts": {
    "ng": "ng",
    "start": "npm run lint && npm run serve:open",
    "build": "ng build",
    "serve": "ng serve",
    "serve:open": "ng serve --open",
    "test": "ng test --code-coverage --watch=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android-build": "ionic cap build android --no-open && ionic cap sync && cd android && gradlew assembleDebug && cd .. && npm run copy-apk-debug",
    "android-build-production": "ionic cap build android --no-open --prod && ionic cap sync && cd android && gradlew assembleRelease && cd .. && npm run copy-apk-release",
    "android-studio-build": "ionic build && ionic cap sync && ionic capacitor build android",
    "android-run": "ionic build && ionic cap run android",
    "android-run-live": "ionic cap run android -l --external",
    "android-open": "ionic cap copy && ionic cap open android",
    "android-open-live": "ionic cap run android --open --livereload --external --host=192.168.68.100",
    "ios-run": "ionic build && ionic cap sync && ionic cap run ios",
    "ios-open": "ionic build && ionic cap sync && ionic cap open ios",
    "generate-icon-ios": "npx cordova-res ios --skip-config --copy",
    "generate-icon-android": "npx cordova-res android --skip-config --copy",
    "icons-capacitor": "capacitor-resources",
    "copy-apk-debug": "copy /v /y .\\android\\app\\build\\outputs\\apk\\debug\\app-debug.apk .\\apk\\portal-coopersystem.apk",
    "copy-apk-release": "copy /v /y .\\android\\app\\build\\outputs\\apk\\release\\app-release-unsigned.apk .\\apk\\portal-coopersystem.apk",
    "pre-push": "npm run lint && npm run test",
    "postinstall": "jetifier"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@capacitor/android": "4.7.0",
    "@capacitor/app": "4.1.1",
    "@capacitor/app-launcher": "^4.1.0",
    "@capacitor/core": "4.7.0",
    "@capacitor/geolocation": "^4.1.0",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/local-notifications": "^4.1.5",
    "@capacitor/network": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@capacitor/screen-orientation": "^4.1.1",
    "@capacitor/status-bar": "^4.1.1",
    "@ionic/angular": "^6.1.9",
    "date-fns": "^2.29.3",
    "ionicons": "^6.0.3",
    "jetifier": "^2.0.0",
    "ol": "^7.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "^15.0.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/schematics": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/language-service": "^15.0.0",
    "@capacitor/cli": "4.7.0",
    "@ionic/angular-toolkit": "^8.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.9.5"
  }
}
