{
  "name": "pontocooper",
  "version": "1.0.8",
  "author": "Cesar HenriQ",
  "description": "Novas funcionalidades, correções de software, Fix localização gps",
  "homepage": "https://portal.coopersystem.com.br/",
  "scripts": {
    "ng": "ng",
    "start": "npm run lint && ng serve --open",
    "build": "ng build",
    "test": "ng test  --code-coverage --watch=true --sourceMap=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android-build": "ionic build && ionic cap sync && ionic capacitor build android",
    "android-run": "ionic build && ionic cap run android",
    "android-open": "ionic build && ionic cap open android",
    "ios-run": "ionic build && ionic cap sync && ionic cap run ios",
    "ios-open": "ionic build && ionic cap sync && ionic cap open ios",
    "android-run-live": "ionic cap run android -l --external",
    "generate-icon-ios": "cordova-res ios --skip-config --copy",
    "generate-icon-android": "cordova-res android --skip-config --copy",
    "icons-capacitor": "capacitor-resources",
    "copy-apk": "copy /v /y .\\android\\app\\build\\outputs\\apk\\debug\\app-debug.apk .\\apk\\portal-coopersystem.apk",
    "pre-push": "npm run lint && npm run test",
    "postinstall": "jetifier"
  },
